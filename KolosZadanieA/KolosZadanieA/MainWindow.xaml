        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KolosZadanieA"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnPudelko" Content="Pudelko" HorizontalAlignment="Left" Height="52" Margin="22,21,0,0" VerticalAlignment="Top" Width="95" Click="btnPudelko_Click"/>
        <ListBox x:Name="listaPudelko" HorizontalAlignment="Left" Height="196" Margin="147,21,0,0" VerticalAlignment="Top" Width="253" d:ItemsSource="{d:SampleData ItemCount=5}"/>

    </Grid>
</Window>
